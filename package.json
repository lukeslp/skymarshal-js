  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lukeslp/skymarshal-js",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/lukeslp/skymarshal-js#readme",
  "bugs": {
    "url": "https://github.com/lukeslp/skymarshal-js/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@atproto/api": "^0.13.0"
  },
  "devDependencies": {

{
  "name": "skymarshal",
  "version": "2.2.0",
  "type": "module",
  "description": "Comprehensive Bluesky/AT Protocol toolkit - authentication, content management, network/graph analysis, thread caching, engagement tracking, deletion workflows, media uploads, chat, analytics, backup, vision AI, and sentiment analysis",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./auth": {
      "types": "./dist/managers/auth.d.ts",
      "import": "./dist/managers/auth.js"
    },
    "./content": {
      "types": "./dist/managers/content.d.ts",
      "import": "./dist/managers/content.js"
    },
    "./network": {
      "types": "./dist/managers/network.d.ts",
      "import": "./dist/managers/network.js"
    },
    "./chat": {
      "types": "./dist/managers/chat.d.ts",
      "import": "./dist/managers/chat.js"
    },
    "./analytics": {
      "types": "./dist/managers/analytics.d.ts",
      "import": "./dist/managers/analytics.js"
    },
    "./search": {
      "types": "./dist/managers/search.d.ts",
      "import": "./dist/managers/search.js"
    },
    "./backup": {
      "types": "./dist/services/backup.d.ts",
      "import": "./dist/services/backup.js"
    },
    "./vision": {
      "types": "./dist/services/vision.d.ts",
      "import": "./dist/services/vision.js"
    },
    "./sentiment": {
      "types": "./dist/services/sentiment.d.ts",
      "import": "./dist/services/sentiment.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    },
    "./models": {
      "types": "./dist/models/index.d.ts",
      "import": "./dist/models/index.js"
    },
    "./notifications": {
      "types": "./dist/managers/notifications.d.ts",
      "import": "./dist/managers/notifications.js"
    },
    "./profile": {
      "types": "./dist/managers/profile.d.ts",
      "import": "./dist/managers/profile.js"
    },
    "./post": {
      "types": "./dist/managers/post.d.ts",
      "import": "./dist/managers/post.js"
    },
    "./lists": {
      "types": "./dist/managers/lists.d.ts",
      "import": "./dist/managers/lists.js"
    },
    "./feeds": {
      "types": "./dist/managers/feeds.d.ts",
      "import": "./dist/managers/feeds.js"
    },
    "./database": {
      "types": "./dist/database/index.d.ts",
      "import": "./dist/database/index.js"
    },
    "./validation": {
      "types": "./dist/utils/validation.d.ts",
      "import": "./dist/utils/validation.js"
    },
    "./image": {
      "types": "./dist/utils/image.d.ts",
      "import": "./dist/utils/image.js"
    },
    "./threads": {
      "types": "./dist/utils/threads.d.ts",
      "import": "./dist/utils/threads.js"
    },
    "./deletion": {
      "types": "./dist/managers/deletion.d.ts",
      "import": "./dist/managers/deletion.js"
    },
    "./engagement": {
      "types": "./dist/managers/engagement.d.ts",
      "import": "./dist/managers/engagement.js"
    },
    "./media": {
      "types": "./dist/managers/media.d.ts",
      "import": "./dist/managers/media.js"
    },
    "./threads": {
      "types": "./dist/utils/threads.d.ts",
      "import": "./dist/utils/threads.js"
    },
    "./graph": {
      "types": "./dist/utils/graph.d.ts",
      "import": "./dist/utils/graph.js"
    },
    "./analytics-utils": {
      "types": "./dist/utils/analytics.d.ts",
      "import": "./dist/utils/analytics.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "check": "tsc --noEmit",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "node --test dist/**/*.test.js"
  },
  "keywords": [
    "bluesky",
    "atproto",
    "at-protocol",
    "social-media",
    "content-management",
    "authentication",
    "search",
    "engagement",
    "analytics",
    "bot-detection",
    "chat",
    "dm",
    "backup",
    "car",
    "vision",
    "alt-text",
    "sentiment",
    "network-analysis",
    "notifications",

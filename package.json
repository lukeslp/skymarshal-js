{
  "name": "skymarshal-core",
  "version": "2.0.0",
  "type": "module",
  "description": "Comprehensive Bluesky/AT Protocol toolkit - authentication, content management, network analysis, chat, analytics, backup, vision AI, and sentiment analysis",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./auth": {
      "types": "./dist/managers/auth.d.ts",
      "import": "./dist/managers/auth.js"
    },
    "./content": {
      "types": "./dist/managers/content.d.ts",
      "import": "./dist/managers/content.js"
    },
    "./network": {
      "types": "./dist/managers/network.d.ts",
      "import": "./dist/managers/network.js"
    },
    "./chat": {
      "types": "./dist/managers/chat.d.ts",
      "import": "./dist/managers/chat.js"
    },
    "./analytics": {
      "types": "./dist/managers/analytics.d.ts",
      "import": "./dist/managers/analytics.js"
    },
    "./search": {
      "types": "./dist/managers/search.d.ts",
      "import": "./dist/managers/search.js"
    },
    "./backup": {
      "types": "./dist/services/backup.d.ts",
      "import": "./dist/services/backup.js"
    },
    "./vision": {
      "types": "./dist/services/vision.d.ts",
      "import": "./dist/services/vision.js"
    },
    "./sentiment": {
      "types": "./dist/services/sentiment.d.ts",
      "import": "./dist/services/sentiment.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    },
    "./models": {
      "types": "./dist/models/index.d.ts",
      "import": "./dist/models/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "check": "tsc --noEmit",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "node --test dist/**/*.test.js"
  },
  "keywords": [
    "bluesky",
    "atproto",
    "at-protocol",
    "social-media",
    "content-management",
    "authentication",
    "search",
    "engagement",
    "analytics",
    "bot-detection",
    "chat",
    "dm",
    "backup",
    "car",
    "vision",
    "alt-text",
    "sentiment",
    "network-analysis",
    "typescript"
  ],
  "author": "Luke Steuber <luke@lukesteuber.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lukeslp/skymarshal-js",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/lukeslp/skymarshal-js#readme",
  "bugs": {
    "url": "https://github.com/lukeslp/skymarshal-js/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@atproto/api": "^0.13.0"
  },
  "devDependencies": {
    "typescript": "^5.6.0"
  },
  "peerDependencies": {
    "@atproto/api": ">=0.12.0"
  }
}
